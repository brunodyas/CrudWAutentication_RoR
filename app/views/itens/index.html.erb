<p id="notice"><%= notice %></p>

<h1>Itens</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Id seq</th>
      <th>Modelo</th>
      <th>Quantidade</th>
      <th>Level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @itens.each do |iten| %>
      <tr>
        <td><%= iten.nome %></td>
        <td><%= iten.id_seq %></td>
        <td><%= iten.modelo %></td>
        <td><%= iten.quantidade %></td>
        <td><%= iten.level %></td>
        <td><%= link_to 'Show', iten %></td>
        <td><%= link_to 'Edit', edit_iten_path(iten) %></td>
        <td><%= link_to 'Destroy', iten, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Iten', new_iten_path %>
